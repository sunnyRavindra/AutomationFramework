<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Framework" verbose="10" thread-count="2" parallel="tests">

	<listeners>
		<listener class-name="listeners.TestListener"></listener>
		<listener class-name="listeners.ReporterListener"></listener>
		<listener class-name="listeners.ExecutionListener"></listener>
		<listener class-name="listeners.InvokedMethodListener"></listener>
		<listener class-name="listeners.SuiteListener"></listener>
	</listeners>

	<test name="DemoQA">
		<classes>
			<class name="runner.SouceDemo"></class>
		</classes>
	</test>

	<test name="SouceDemo">
		<classes>
			<class name="runner.SouceDemo"></class>
		</classes>
	</test>

</suite> 
